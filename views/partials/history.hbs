<div  id="oldResev">
      <div class="tasks">
          <h1>Old Reservations</h1>
          
            {{!-- //TODO Create and position the title of each column --}}
            {{#if info}}
            <ul class="task-list" style="overflow-y: scroll; max-height: 200px;"></ul>
            {{#each info}}
            <li>
              <span>{{reservID}}</span>
              <span>{{formatDateMMDDYYYY date}}</span>
              <span>{{time}}</span>
              <span>{{numofpeople}}</span>
              {{!-- <a class="btn btn-primary review" href="/review/{{reservID}}">Review</a> --}}
              </li>
              <hr>
            {{/each}}
          </ul>
            {{else}}
            <div style="color:gray;">No reservations to show</div>
            {{/if}}
            
            
          
      </div>
    </div>